{{ include(_self|replace({ "content.html.twig": baks_settings.device~"/content.html.twig" })) }}

<script nonce='{{ csp_nonce() }}' async data-src="/assets/{{ version }}/support/support.js?v={{ version }}"
        class="lazy"></script>
<script nonce='{{ csp_nonce() }}' async data-src="/assets/{{ version }}/js/copy.min.js?v={{ version }}"
        class="lazy"></script>
<script nonce='{{ csp_nonce() }}' async
        data-src='/assets/{{ version }}/plugins/nice-select2/nice-select2.min.js?v={{ version }}'
        class='lazy'></script>

<script nonce="{{ csp_nonce() }}" class="lazy">
    window.centrifugo_dsn = "{{ centrifugo_dsn }}";
    window.centrifugo_token = "{{ token }}";
    window.current_profile = "{{ baks_profile.current }}";
</script>